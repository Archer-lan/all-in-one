.container {
    padding: 20rpx;
    background-color: #f5f5f5;
    height: 100vh;
  }
  
  .header {
    font-size: 36rpx;
    font-weight: bold;
    color: #333;
    margin-bottom: 20rpx;
  }
  
  .upload-section {
    display: flex;
    justify-content: center;
    margin-bottom: 20rpx;
  }
  
  .upload-btn {
    background-color: #007aff;
    color: #fff;
    padding: 20rpx;
    font-size: 30rpx;
    border-radius: 10rpx;
    text-align: center;
  }
  
  .photo-list {
    flex: 1;
    padding: 10rpx;
    background-color: #fff;
    border-radius: 10rpx;
    box-shadow: 0 2rpx 5rpx rgba(0, 0, 0, 0.1);
  }
  
  .photo-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 20rpx;
  }
  
  .photo {
    width: 100%;
    height: 300rpx;
    border-radius: 10rpx;
    box-shadow: 0 2rpx 5rpx rgba(0, 0, 0, 0.1);
  }
  
  .photo-actions {
    margin-top: 10rpx;
    display: flex;
    justify-content: flex-end;
    width: 100%;
  }
  
  .delete-btn {
    color: #ff4d4f;
    font-size: 28rpx;
    margin-right: 10rpx;
  }
  